{
  "name": "remysabliet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development node scripts/start.js webpack --mode development ",
    "start-prod": "cross-env NODE_ENV=production node scripts/start.js",
    "build": "node scripts/build.js webpack -p",
    "test-coverage": "node scripts/test.js --env=jsdom --coverage",
    "lint": "eslint ./src --ext .jsx,.js",
    "test": "node scripts/test.js --env=jsdom",
    "doc": "jsdoc src -r -d docs",
    "precommit": "lint-staged",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check"
  },
  "dependencies": {
    "@types/react": "^16.7.21",
    "@webcomponents/webcomponentsjs": "^2.1.3",
    "ajv-keywords": "^3.2.0",
    "autoprefixer": "^9.3.1",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bootstrap": "^4.1.1",
    "canvas-prebuilt": "^1.6.11",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "cross-env": "^5.2.0",
    "css-to-string-loader": "^0.1.3",
    "d3": "^5.7.0",
    "d3-array": "^1.2.4",
    "d3-scale": "^2.1.2",
    "d3-selection": "^1.3.2",
    "d3-timer": "^1.0.9",
    "fs": "0.0.1-security",
    "gsap": "^2.0.2",
    "history": "^4.7.2",
    "husky": "^1.2.0",
    "is_js": "^0.9.0",
    "jest": "^23.6.0",
    "konva": "^2.3.0",
    "loose-envify": "^1.4.0",
    "node-sass": "^4.11.0",
    "path": "^0.12.7",
    "postcss-flexbugs-fixes": "^4.1.0",
    "prettier": "^1.16.3",
    "prop-types": "^15.6.2",
    "react": "^16.8.1",
    "react-dev-utils": "^6.1.1",
    "react-dev-utils-for-webpack4": "^5.0.0-webpack4",
    "react-dom": "^16.4.2",
    "react-intl": "^2.4.0",
    "react-redux": "^5.1.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-hash-link": "^1.2.0",
    "react-scripts": "1.1.5",
    "react-shadow": "^16.3.0",
    "react-testing-library": "^5.0.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.0",
    "sass": "^1.17.0",
    "sass-loader": "^7.1.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url": "^0.11.0",
    "webpack-dev-server": "^3.1.10",
    "webpack-manifest-plugin": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.10.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-react-redux": "^2.2.0",
    "extract-text-webpack-plugin": "v4.0.0-alpha.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^4.0.0-beta.4",
    "image-webpack-loader": "^4.3.1",
    "mini-css-extract-plugin": "^0.4.2",
    "serviceworker-webpack-plugin": "^1.0.0-alpha02",
    "webcomponents.js": "~0.7.24",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^src(.*)$": "<rootDir>/src$1"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jest/jestSetUp.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "lint-staged": {
    "src/**/*.js*": [
      "eslint --fix",
      "git add"
    ]
  }
}
